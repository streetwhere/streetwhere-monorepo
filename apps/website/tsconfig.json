{
	"extends": "@streetwhere/typescript-config/base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": ["./app/*"]
		},
		"plugins": [{ "name": "next" }],
		"tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
		"incremental": true
	},
	"include": [".", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
